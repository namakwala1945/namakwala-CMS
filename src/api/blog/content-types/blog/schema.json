{
  "kind": "collectionType",
  "collectionName": "blogs",
  "info": {
    "singularName": "blog",
    "pluralName": "blogs",
    "displayName": "Blogs"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "title": {
      "type": "string"
    },
    "slug": {
      "type": "uid",
      "targetField": "title"
    },
    "Excerpt": {
      "type": "text"
    },
    "content": {
      "type": "blocks"
    },
    "CoverImage": {
      "type": "media",
      "multiple": false,
      "allowedTypes": [
        "images",
        "files",
        "videos",
        "audios"
      ]
    },
    "Metadata": {
      "type": "component",
      "component": "seo.metadata",
      "repeatable": false
    },
    "pagebanner": {
      "type": "component",
      "component": "banner.banner",
      "repeatable": false
    },
    "AuthorName": {
      "type": "string"
    },
    "PublishedDate": {
      "type": "datetime"
    }
  }
